{
  "name": "DOM manipulation Example",
  "version": "1.0",
  "description": "Chrome Extension Example for manipulating DOM.",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Title Changer"
  },
  "permissions": [
    "webNavigation",
    "*://*/*"
  ],
  "content_scripts": [    
       {
           "matches": [
               "http://*/*",
               "https://*/*"
           ],
           "js": ["content.js"],
           "all_frames": true,
           "run_at": "document_idle"
       }
   ],
  "manifest_version": 2
}